<div class="modal-background">
</div>

<div class="modal-raised">
  <div class="modal upload-modal" id="track-upload">
    <div class="modal-wrapper group">
      <div class="modal-header">

        <div class="modal-header-text"><h1>Upload to TrackStack</h1></div>

      </div>

      <section class="tab-content group">
        <article id="signin-content" class="active tab-content-item">
          <form class="file-input-plugin">
            <label class="input-label">Title</label>
            <input class="form-field" type="text" name="track[title]" id="track_title">
            <label class="input-label">Description</label>
            <input class="form-field" type="text" name="track[description]" id="track_description">

            <div class="input-hider">
              <input class="file-input file-input-button" data-form-type="#track-upload" id="track-file-input-button" type="file" name="track[track]">
            </div>
            <button class="button button-orange submit upload-submit">Save</button>
          <button class="button button orange css-file-input">Choose file to upload</button>
          </form>


        </article>
      </section>
    </div>
  </div>
</div>
